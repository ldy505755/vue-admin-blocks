{
  "name": "vue-admin-blocks",
  "version": "3.2.0",
  "author": "liudanyun <ldy505755@icloud.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "test": "vue-cli-service build --mode test",
    "release": "vue-cli-service build --mode release",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "countup.js": "^1.9.3",
    "iview": "^3.2.2",
    "vue": "^2.5.21",
    "vue-countup-v2": "^1.0.3",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.1.0",
    "mockjs": "^1.0.1-beta3",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off",
      "vue/no-parsing-error": "off",
      "indent": [
        "off",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "off",
        "always"
      ]
    }
  },
  "postcss": {
    "plugins": {
      "postcss-cssnext": {},
      "postcss-import": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}